function [] = handle_theory_result(experiment_num)
    switch experiment_num
        case 1
            load '../output data of two stage experiment/Adult/test_accuracy.mat';
            num = 10;
        case 2
            load '../output data of two stage experiment/Conect/test_accuracy.mat';
            num = 12;
        case 3
            load '../output data of two stage experiment/mushroom/test_accuracy.mat';
            num = 4;
        case 4
            load '../output data of two stage experiment/test_data/test_accuracy.mat';
            num = 2;
        case 5
            load '../output data of two stage experiment/Real Data/test_accuracy.mat';
            num = 1;
        case 6
            load '../output data of two stage experiment/Real_Data_AAAI_balance/test_accuracy.mat';
            num = 1;
    end 
%     [m,n] = size(test_accuracy);
%     x = (1:n);
%     figure()
%     hold on
%     for i = 1:m
%         plot(x,test_accuracy(i,:));
%     end
    

    linewidth=1;
    MarkerSize = 6;
    figure();
    hold on
    han(1,1)=plot( test_accuracy(1,:),':ok','LineWidth',linewidth,'MarkerFaceColor','w','MarkerSize',MarkerSize);
    han(2,1)=plot( test_accuracy(2,:),'-.xk','LineWidth',linewidth,'MarkerFaceColor','w','MarkerSize',MarkerSize);
    han(3,1)=plot( test_accuracy(3,:),'--^k','LineWidth',linewidth,'MarkerFaceColor','w','MarkerSize',MarkerSize);
    han(4,1)=plot( test_accuracy(4,:),'-ok','LineWidth',linewidth,'MarkerFaceColor','w','MarkerSize',MarkerSize);
    han(5,1)=plot( test_accuracy(5,:),':xk','LineWidth',linewidth,'MarkerFaceColor','w','MarkerSize',MarkerSize);
    han(6,1)=plot( test_accuracy(6,:),'-.^k','LineWidth',linewidth,'MarkerFaceColor','w','MarkerSize',MarkerSize);
    han(7,1)=plot( test_accuracy(7,:),'--ok','LineWidth',linewidth,'MarkerFaceColor','w','MarkerSize',MarkerSize);
    han(8,1)=plot( test_accuracy(8,:),'-xk','LineWidth',linewidth,'MarkerFaceColor','w','MarkerSize',MarkerSize);
    han(9,1)=plot( test_accuracy(9,:),':^k','LineWidth',linewidth,'MarkerFaceColor','w','MarkerSize',MarkerSize);
    han(10,1)=plot(test_accuracy(10,:),'-.ok','LineWidth',linewidth,'MarkerFaceColor','w','MarkerSize',MarkerSize);    
    han(11,1)=plot(test_accuracy(11,:),'--xk','LineWidth',linewidth,'MarkerFaceColor','w','MarkerSize',MarkerSize); 
    le1=legend(han(1:5),'p=0','p=0.1','p=0.2');
    set(le1,'Box','off');
    ah=axes('position',get(gca,'position'),'visible','off');
    le2=legend(ah,han(6:11),'PMActive','IEThresh');
    set(le2,'Box','off');
%     temp=[0 10 100];
%     set(gca,'YTick',log(1+temp)/log(10))
%     set(gca,'YTickLabel',{'0','10','100'})    
%     axis([0 len 0 2.1])                 
    
end
